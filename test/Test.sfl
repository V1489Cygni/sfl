module Test

data List = Empty | Cons Int List

put :: Int -> List -> List
put a b = Cons a b

top :: List -> Int
top (Cons a b) = a

get :: List -> Int -> Int
get (Cons a b) 0 = a
get (Cons a b) n = get b (- n 1)

remove :: List -> List
remove (Cons a b) = b

main :: Int
main = get (put 3 (remove (put 2 (put 7 (put 1 Empty))))) 1
